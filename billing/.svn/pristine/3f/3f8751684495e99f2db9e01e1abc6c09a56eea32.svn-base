package com.tydic.beijing.billing.dto;

import java.io.Serializable;

/**
 * 
 * @author Tian
 *
 */
public class RechargeInfoDto implements Serializable {
	private static final long serialVersionUID = 1L;

	private String RechargeType; // RechargeType
	private long Amount; // Amount

	// added by tian@20150505 for: B值兑换话费用
	private String EffDate; // yyyymmddhh24miss
	private String ExpDate; // yyyymmddhh24miss

	public String getRechargeType() {
		return RechargeType;
	}

	public void setRechargeType(String rechargeType) {
		RechargeType = rechargeType;
	}

	public long getAmount() {
		return Amount;
	}

	public void setAmount(long amount) {
		Amount = amount;
	}

	public String getEffDate() {
		return EffDate;
	}

	public void setEffDate(String effDate) {
		EffDate = effDate;
	}

	public String getExpDate() {
		return ExpDate;
	}

	public void setExpDate(String expDate) {
		ExpDate = expDate;
	}

	@Override
	public String toString() {
		return "RechargeInfoDto [RechargeType=" + RechargeType + ", Amount="
				+ Amount + ", EffDate=" + EffDate + ", ExpDate=" + ExpDate
				+ "]";
	}
}
