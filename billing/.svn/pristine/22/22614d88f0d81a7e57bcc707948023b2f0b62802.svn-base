package com.tydic.beijing.billing.dto;

/**
 *
 * @author wangshida
 */
public class AmountDto implements java.io.Serializable {
	private static final long serialVersionUID = 1L;
	private Long Amounttype;
    private Long Amount;

    public AmountDto(Long Amounttype, Long Amount) {
        this.Amounttype = Amounttype;
        this.Amount = Amount;
    }
    
    public Long getAmounttype() {
        return Amounttype;
    }

    public void setAmounttype(Long Amounttype) {
        this.Amounttype = Amounttype;
    }

    public Long getAmount() {
        return Amount;
    }

    public void setAmount(Long Amount) {
        this.Amount = Amount;
    }

    @Override
    public String toString() {
        return "AmountDto{" + "Amounttype=" + Amounttype + ", Amount=" + Amount + '}';
    }
}
