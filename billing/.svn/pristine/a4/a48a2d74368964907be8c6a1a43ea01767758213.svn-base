<?xml version="1.0" encoding="UTF-8"?>
<vds>
	<drivers>
		<driver id="1" clazz="oracle.jdbc.driver.OracleDriver"
			url="jdbc:oracle:thin:@172.168.1.246:1521:bssgx" user="newbilling"
			password="newbilling">
			<active>
				<tables schema="NEWBILLING"
					filter="table_name in( 
					'CREDIT_PARA' ,
					'CREDIT_PROC_PARA',
					'CREDIT_ACTION',
					'CODE_ACCT_MONTH',
					'Q_BLOCK',
					'BIL_ACT_BILL' ,
					'CREDIT_SEQUENCE_UTILS' ,
					'LOG_CREDIT_ACTION',
					'HLP_SMS_SEND',
					'LOG_Q_BLOCK',
					'Q_USER_REASON_SEND_PRE',
					'LOG_CREDIT_FILE' ) or 
					table_name like 'BIL_ACT_REAL_TIME_BILL%' or
					table_name like 'BIL_ACT_RESOURCE_ADD_UP%' or
                    table_name like 'LOG_CREDIT_FLOW_OVERTOP'
					  " />
			</active>
		</driver>

		<driver id="2" clazz="oracle.jdbc.driver.OracleDriver"
			url="jdbc:oracle:thin:@172.168.1.246:1521:bssgx" user="newbilling"
			password="newbilling">
			<active>
