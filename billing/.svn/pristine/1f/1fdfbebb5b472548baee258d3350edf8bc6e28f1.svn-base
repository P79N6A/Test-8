package com.tydic.beijing.billing.rating.domain;

public class MTChangeInfo {
	public   int  nMTMode;				//0 – 非MT计费,1 – M计费, 2 – T 计费
	public   int  nMTCount;				//扣减MT的数量
	public   long lnMTChangeMoney;		//扣减MT转换金钱的数量
	public   long lnExMoney;             //MT附加金额数量

