package com.tydic.beijing.billing.ua.dao;

import java.io.Serializable;
import java.sql.Date;

/**
 * 判断号码类别 <br/>
 * 0 固话， 1 移动<br/>
 * 
 * @author Bradish7Y
 * @since JDK 1.7
 * 
 */
public class SmCodeNumberClass implements Serializable {

	private static final long serialVersionUID = 1L;

	public String number_prefix;
	public String number_class;
	public Date eff_date;
	public Date exp_date;

	public final String getNumber_prefix() {
		return number_prefix;
	}

	public final void setNumber_prefix(String number_prefix) {
		this.number_prefix = number_prefix;
	}

	public final String getNumber_class() {
		return number_class;
	}

	public final void setNumber_class(String number_class) {
		this.number_class = number_class;
	}

	public final Date getEff_date() {
		return eff_date;
	}

	public final void setEff_date(Date eff_date) {
		this.eff_date = eff_date;
	}

	public final Date getExp_date() {
		return exp_date;
	}

	public final void setExp_date(Date exp_date) {
		this.exp_date = exp_date;
	}

	@Override
	public String toString() {
		return "SmCodeNumberClass [number_prefix=" + number_prefix + ", number_class="
				+ number_class + ", eff_date=" + eff_date + ", exp_date=" + exp_date + "]";
	}

}
