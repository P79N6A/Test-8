package com.tydic.beijing.billing.rating.domain;

public class RBConf {

	private int tariffChangeFlag=0;  //费率切换开关
	private int directRatableFlag=0;
	
	public int getTariffChangeFlag() {
		return tariffChangeFlag;
	}
	
	public void setTariffChangeFlag(int tariffChangeFlag) {
		this.tariffChangeFlag = tariffChangeFlag;
	}
	
	public int getDirectRatableFlag() {
		return directRatableFlag;
	}
	
	public void setDirectRatableFlag(int directRatableFlag) {
		this.directRatableFlag = directRatableFlag;
	}
	
	
	
}
