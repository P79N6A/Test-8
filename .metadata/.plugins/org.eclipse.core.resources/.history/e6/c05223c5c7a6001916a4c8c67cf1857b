		<service code="account.batchcycle.lifeUserProduct" clazz="com.tydic.vds.service.single.CommonService" isQuery="true">
			<script>
			<![CDATA[--@DataSource(2)
			SELECT DISTINCT T1.*
          FROM LIFE_USER_PRODUCT T1
         WHERE T1.USER_ID = ?
           AND T1.PRODUCT_ID IN
               (SELECT DISTINCT T4.PRODUCT_ID
                  FROM BILLING.RULE_OFR_SPLIT           T4,
                       BILLING.RULE_OFR_TARIFF_RELATION T5,
                       BILLING.RULE_TARIFF_CONF_INFO    T6
                 WHERE T4.OFR_C_ID = T5.OFR_C_ID
                   AND T5.TARIFF_ID = T6.TARIFF_ID
                   AND T6.EVENT_TYPE_ID = ?)
           AND to_date(?,'yyyymmddhh24miss') BETWEEN T1.EFF_DATE AND T1.EXP_DATE]]>
			</script>
		</service>
		<service code="account.restoacct.lifeUserProduct" clazz="com.tydic.vds.service.single.CommonService" isQuery="true">
			<script>
			<![CDATA[
			--@DataSource(1)
			SELECT distinct T2.*  FROM RULE_PRODUCT_RESOURCE T1, CRM_USER.LIFE_USER_PRODUCT T2
			 WHERE T1.PRODUCT_ID = T2.PRODUCT_ID AND t2.user_id = ?
   			AND T2.EFF_FLAG = 0   AND to_date(?,'yyyymmddhh24miss') BETWEEN T2.EFF_DATE AND T2.EXP_DATE]]>
			</script>
		</service>
			
