package test;
class Value {
    int v;
    public Value(int v) {
        this.v = v;
    }
}

public class Demo {
    
    final int f1 = 1;
    final int f2;
    public Demo() {
        f2 = 2;
    }

    public static void main(String[] args) {
        final int value1 = 1;
        // value1 = 4;
        final double value2;
        value2 = 2.0;
        final Value value3 = new Value(1);
        value3=new Value(3);
        value3.v = 4;
    }
}